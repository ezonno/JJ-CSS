#footer_global
	margin-top 0

#branded .content
	background #f5f5f5

	// v0.5 overwrites
	.storefront-promos
	.storefront-brands-callout
	.storefront-brands
	.storefront-sellingpoints
	.storefront-categories-callout
		display none

	// Logo
	.storefront-logo
		height 70px
		background #fff url('data:image/png;base64,') center no-repeat

	// Hero
	.hero
		overflow hidden
		position relative
		text-align center
		width 100%

		.swiper-prev
		.swiper-next
			height 122px
			margin-top -61px
			opacity 0.7
			position absolute
			top 50%
			width 60px
			z-index 99
			transition all 0.2s ease-out

		.swiper-prev
			background url('../img/swiperArrowLeft.png') no-repeat center top
			cursor pointer
			left 2%

			&:hover
				left 1.8%
				opacity 1

			&.background--light
				background-image url('../img/swiperArrowLeft-black.png')

		.swiper-next
			background url('../img/swiperArrowRight.png') no-repeat center bottom
			cursor pointer
			right 2%

			&:hover
				opacity 1
				right 1.8%

			&.background--light
				background-image url('../img/swiperArrowRight-black.png')

		.swiper-pagination
			bottom 0
			left 0
			padding 15px 0
			position absolute
			text-align center
			width 100%
			z-index 900

		.swiper-pagination-switch
			background #FFF
			border-radius 10px
			box-shadow 0px 1px 1px #666
			cursor pointer
			display inline-block
			height 9px
			margin 0 5px
			opacity 0.3
			width 9px
			transition all 0.2s ease-out

			&:hover
				opacity 0.8

		.swiper-active-switch
		.swiper-active-switch:hover
			opacity 1

		.background--light .swiper-pagination-switch
			background-color black

		.swiper-slide
			display none

			&.first-slide
				display block

			img
				border 0
				padding 0
				position relative
				width 100%

			a
				display block

	.brandsite-callout
		text-align center
		background #fff
		
	.brandsite-content-boxes
		width 900px //880px + 20px margin
		margin 10px auto 0
		
		.content-box
			float left
			background #fff
			padding 15px
			margin 10px
			
	.brandsite-full-width-content-box
		position relative
		margin-top 40px

		.content-box
			background #fff
			padding 15px 0

			img
				&.background
					width 100%
				
				&.floater
					position absolute
					top 50%
					left 50%
					transform translateX(-50%) translateY(-50%)
					
	.brandsite-small-content-boxes
		width 900px //880px + 20px margin
		margin 10px auto 0
		
		.content-box
			float left
			background #fff
			padding 15px
			margin 10px
	
	.brandsite-footer-logo
		height 150px
		background #fff url('data:image/png;base64,') no-repeat center
		margin-top 40px